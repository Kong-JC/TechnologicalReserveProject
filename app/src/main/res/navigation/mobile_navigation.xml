<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/mainFragment">
    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.technologicalreserveproject.MainFragment"
        android:label="MainFragment" >
        <action
            android:id="@+id/action_mainFragment_to_addDelItemListFragment"
            app:destination="@id/addDelItemListFragment"
            app:enterAnim="@anim/fragment_slide_right_in"
            app:exitAnim="@anim/fragment_slide_left_out"
            app:popEnterAnim="@anim/fragment_slide_left_in"
            app:popExitAnim="@anim/fragment_slide_right_out"
            />
        <action
            android:id="@+id/action_mainFragment_to_signatureFragment"
            app:destination="@id/signatureFragment"
            app:enterAnim="@anim/fragment_slide_right_in"
            app:exitAnim="@anim/fragment_slide_left_out"
            app:popEnterAnim="@anim/fragment_slide_left_in"
            app:popExitAnim="@anim/fragment_slide_right_out"
            />
    </fragment>
    <fragment
        android:id="@+id/addDelItemListFragment"
        android:name="com.example.technologicalreserveproject.addDelItemList.AddDelItemListFragment"
        android:label="AddDelItemListFragment" />
    <fragment
        android:id="@+id/signatureFragment"
        android:name="com.example.technologicalreserveproject.signatureModel.SignatureFragment"
        android:label="SignatureFragment" />
</navigation>
